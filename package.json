  "devDependencies": {
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.0-beta.6",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.7.2",
    "check-dependencies": "0.12.0",
    "chokidar": "1.4.3",
