{
  "name": "tofino",
  "version": "0.1.0",
  "description": "Experimental prototype",
  "main": "lib/main/browser.js",
  "repository": "https://github.com/mozilla/tofino",
  "_electron": {
    "version": "0.37.8"
  },
  "scripts": {
    "start": "node build/main.js --run",
    "dev": "node build/main.js --run-dev",
    "package": "node build/main.js --package",
    "test": "nyc -s node build/main.js --test",
    "quicktest": "nyc node build/main.js --quicktest",
    "lint": "nyc node build/main.js --lint",
    "report": "nyc report",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "install": "node build/main.js --build-deps",
    "postinstall": "node build/main.js --build"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "babel-polyfill": "6.7.4",
    "body-parser": "1.15.1",
    "electron-localshortcut": "0.6.0",
    "express": "4.13.4",
    "express-validator": "2.20.4",
    "express-ws": "^2.0.0-rc.1",
    "immutable": "3.7.6",
    "less-mkdirp": "1.0.1",
    "lodash": "4.12.0",
    "microtime-fast": "1.0.2",
    "morgan": "1.7.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-free-style": "2.1.1",
    "react-immutable-proptypes": "1.7.1",
    "react-redux": "4.4.5",
    "redux": "3.5.1",
    "redux-immutable": "3.0.6",
    "redux-logger": "2.6.1",
    "request": "2.71.0",
