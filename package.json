    "eslint-plugin-import": "1.4.0",
    "eslint-plugin-react": "4.2.3",
    "expect": "1.18.0",
    "extract-zip": "1.5.0",
    "fetch-mock": "4.4.0",
    "fs-promise": "0.5.0",
    "glob": "7.0.3",
    "gulp-vinyl-zip": "1.2.2",
    "jsdom": "8.1.0",
    "mocha": "2.4.5",
    "mocha-eslint": "2.0.2",
    "nyc": "6.4.0",
